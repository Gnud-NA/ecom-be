    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "build:stg": "NODE_OPTIONS=\"--max_old_space_size=768\" nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:stg": "NODE_OPTIONS=\"--max_old_space_size=768\" nest start",
        "dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --no-cache --runInBand --detectOpenHandles --forceExit --testTimeout 100000",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:cli": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "migration": "env-cmd -f ./.env npx sequelize-cli migration:generate --name",
        "migrate": "env-cmd -f ./.env npx sequelize-cli db:migrate",
        "migrate:undo": "env-cmd -f ./.env npx sequelize-cli db:migrate:undo",
        "make:seed": "env-cmd -f ./.env npx sequelize-cli seed:generate --name",
        "seed:all": "env-cmd -f ./.env npx sequelize-cli db:seed:all",
        "seed": "env-cmd -f ./.env npx sequelize-cli db:seed --seed",
        "seed:undo": "env-cmd -f ./.env npx sequelize-cli db:seed:undo"
    },
